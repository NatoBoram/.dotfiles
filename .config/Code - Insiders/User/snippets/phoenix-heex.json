{
	"Get Text": {
		"prefix": ["gettext"],
		"body": ["<%= gettext \"$1\" %>", "$0"],
		"description": "Adds a gettext call to the template"
	},
	"IO Inspect": {
		"prefix": ["|> IO", "ioi"],
		"body": ["|> IO.inspect(label: \"${1/(.*)/${1:/upcase}/}\")"],
		"description": "Appends `|> IO.inspect` with a label."
	},
	"SVG Icon": {
		"prefix": ["svg_icon", "<%= svg_icon"],
		"body": ["<%= svg_icon \"$1\" %>", "$0"],
		"description": "Adds an svg_icon to the template"
	}
}
