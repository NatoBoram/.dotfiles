[
	{
		"args": { "text": "\\\r\n" },
		"command": "workbench.action.terminal.sendSequence",
		"key": "shift+enter",
		"when": "terminalFocus"
	},
	{
		"args": {
			"commands": [
				{ "command": "editor.action.clipboardCopyAction" },
				{ "command": "editor.action.insertLineAfter" },
				{
					"args": { "langId": "typescript", "name": "Log copied variable" },
					"command": "editor.action.insertSnippet"
				}
			],
			"langId": "typescript",
			"name": "Log copied variable"
		},
		"command": "runCommands",
		"key": "ctrl+k l",
		"when": "editorTextFocus"
	},
	{
		"command": "-bookmarks.jumpToNext",
		"key": "ctrl+alt+l",
		"when": "editorTextFocus"
	},
	{
		"command": "-bookmarks.jumpToPrevious",
		"key": "ctrl+alt+j",
		"when": "editorTextFocus"
	},
	{ "command": "-editor.action.codeAction", "key": "ctrl+alt+i" },
	{ "command": "-workbench.action.quit", "key": "ctrl+q" },
	{
		"command": "bookmarks.listFromAllFiles",
		"key": "ctrl+alt+l",
		"when": "editorTextFocus"
	},
	{ "command": "deleteRight", "key": "delete", "when": "textInputFocus" },
	{ "command": "editor.action.inlineSuggest.trigger", "key": "alt+c" },
	{
		"command": "editor.action.sortLinesAscending",
		"key": "alt+s l",
		"when": "editorHasSelection && !editorReadonly"
	},
	{
		"args": { "commands": ["json.sort", "editor.action.formatDocument"] },
		"command": "runCommands",
		"key": "alt+s j",
		"when": "(editorLangId == 'json' || editorLangId == 'jsonc' || editorLangId == 'snippets') && !editorReadonly"
	},
	{
		"args": {
			"commands": ["vscode-yaml-sort.sortYaml", "editor.action.formatDocument"]
		},
		"command": "runCommands",
		"key": "alt+s y",
		"when": "(editorLangId == 'yaml' || editorLangId == 'dockercompose' || editorLangId == 'github-actions-workflow') && !editorReadonly"
	},
	{
		"command": "workbench.action.nextEditor",
		"key": "ctrl+tab",
		"when": "inEditorsPicker && inQuickOpen"
	},
	{
		"command": "workbench.action.nextEditorInGroup",
		"key": "ctrl+tab",
		"when": "!activeEditorGroupEmpty"
	},
	{
		"command": "workbench.action.previousEditor",
		"key": "ctrl+shift+tab",
		"when": "inEditorsPicker && inQuickOpen"
	},
	{
		"command": "workbench.action.previousEditorInGroup",
		"key": "ctrl+shift+tab",
		"when": "!activeEditorGroupEmpty"
	},
	{
		"command": "tsco.organize",
		"key": "alt+s o",
		"when": "editorLangId == 'typescript' && !editorReadonly"
	}
]
